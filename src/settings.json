{
    "Network": {
        "RaspberryPiIP": "192.168.0.131",
        "RaspberryPiPort": 1883,
        "MqttTopicWindows": "moosinator/windows",
        "MqttTopicPi": "moosinator/pi"
    },
    "Image": {
        "RotateIRImage": 180,
        "TargetFPS": 30,
        "TargetResolution": [
            480,
            360
        ],
        "Type": "webcam"
    },
    "ObjectDetection": {
        "MinimumConfidence": 80,
        "ValidTargetLabels": [
            "person"
        ]
    },
    "Turret": {
        "AiTurretControl": true,
        "HumanTurretControl": false,
        "TurretPanBoundNegative": 0,
        "TurretPanBoundPositive": 180,
        "TurretTiltBoundNegative": 0,
        "TurretTiltBoundPositive": 180,
        "MinimumTimeBetweenCommands": 0.5,
        "TurretDampeningFactor": 1.4
    },
    "Debug": {
        "PrintMqttFPS": false,
        "PrintMqttMessageSent": false,
        "PrintTargetingBuffer": false,
        "PrintFilteredTargetingBuffer": false,
        "PrintTurretCommand": true,
        "PrintMessagesFromArduino": true
    }
}